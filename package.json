    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^20.10.4",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "sass": "^1.70.0",
    "typescript": "^5.3.3"
  }
}
